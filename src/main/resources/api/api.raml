#%RAML 1.0
title: Customer-sapi-raml
description: This API is used by PAPI to connect with SFDC.
version: 1.0.0
traits: 
  common-header: !include /exchange_modules/6f2e8f19-043c-4d17-bcf4-5a8154de12f0/common-fragment/1.0.0/traits/header-common-trait.raml
types:
  error-datatType: !include /exchange_modules/6f2e8f19-043c-4d17-bcf4-5a8154de12f0/common-fragment/1.0.0/dataTypes/error-dataType.raml
  post-registration-request: !include /exchange_modules/6f2e8f19-043c-4d17-bcf4-5a8154de12f0/common-fragment/1.0.0/dataTypes/post-registration-request-dataType.raml
  post-registration-response: !include /exchange_modules/6f2e8f19-043c-4d17-bcf4-5a8154de12f0/common-fragment/1.0.0/dataTypes/post-registration-response-dataType.raml
/customers:
  /registration:
    post:
      is:
        - common-header
      queryParameters:
          featureName:
            type: string
            required: true
            enum:
              - cf
              - cwf
            example: cf
      body:
        application/json:
          type:  post-registration-request              
          example: !include /examples/post-registration-request-example.json
              
      responses:
        201:
            body:
                application/json:
                  type: post-registration-response
                  example: !include /examples/post-registration-response-example.json
                              
        400:
          body:
            application/json:
              type: error-datatType
              example: !include /exchange_modules/6f2e8f19-043c-4d17-bcf4-5a8154de12f0/common-fragment/1.0.0/examples/error-400-response-example.json
                
        401:
           body:
            application/json:
              type: error-datatType
              example: !include /exchange_modules/6f2e8f19-043c-4d17-bcf4-5a8154de12f0/common-fragment/1.0.0/examples/error-401-response-example.json
                
                
        404:
           body:
            application/json:
              type: error-datatType
              example: !include /exchange_modules/6f2e8f19-043c-4d17-bcf4-5a8154de12f0/common-fragment/1.0.0/examples/error-404-response-example.json
                
        500:
           body:
            application/json:
              type: error-datatType
              example: !include /exchange_modules/6f2e8f19-043c-4d17-bcf4-5a8154de12f0/common-fragment/1.0.0/examples/error-500-response-example.json
                

                    
                    

                    
                                
                       
                        
        
            